<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@drawable/bottom_sheet_dialog_bg">    <View        android:id="@+id/closing_view"        android:layout_width="40dp"        android:layout_height="6dp"        android:layout_marginTop="16dp"        android:background="@drawable/view_bg"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent" />    <View        android:id="@+id/where_view"        android:layout_width="match_parent"        android:layout_height="100dp"        android:layout_marginHorizontal="16dp"        android:layout_marginTop="24dp"        android:background="@drawable/rounded_corner"        app:layout_constraintTop_toBottomOf="@id/closing_view" />    <ImageView        android:id="@+id/airplane_image_view"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="18dp"        android:layout_marginTop="20dp"        android:src="@drawable/ic_airplane2"        app:layout_constraintStart_toStartOf="@id/where_view"        app:layout_constraintTop_toTopOf="@id/where_view" />    <TextView        android:id="@+id/from_country_text_view"        style="@style/ButtonText1"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="8dp"        android:textColor="@color/white"        app:layout_constraintStart_toEndOf="@id/airplane_image_view"        app:layout_constraintTop_toTopOf="@id/airplane_image_view"        tools:text="Минск" />    <View        android:id="@+id/center_view"        android:layout_width="0dp"        android:layout_height="1dp"        android:layout_marginHorizontal="16dp"        android:layout_marginTop="8dp"        android:background="@color/grey_3"        app:layout_constraintEnd_toEndOf="@id/where_view"        app:layout_constraintStart_toStartOf="@id/where_view"        app:layout_constraintTop_toBottomOf="@id/airplane_image_view" />    <ImageView        android:id="@+id/search_image_view"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="12dp"        android:src="@drawable/ic_search"        app:layout_constraintStart_toStartOf="@id/airplane_image_view"        app:layout_constraintTop_toBottomOf="@id/center_view" />    <EditText        android:id="@+id/destination_edit_text"        style="@style/ButtonText1"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="8dp"        android:background="@android:color/transparent"        android:digits="абвгдеёжзийклмнопрстуфхцчшщъыьэюяАБВГДЕЁЖЗИЙКЛМНОПРСТУФХЦЧШЩЪЫЬЭЮЯ"        android:hint="Куда - Турция"        android:imeOptions="actionDone"        android:inputType="text"        android:singleLine="true"        android:textColor="@color/white"        android:textColorHint="@color/grey_5"        app:layout_constraintBottom_toBottomOf="@id/search_image_view"        app:layout_constraintStart_toEndOf="@id/search_image_view"        app:layout_constraintTop_toTopOf="@id/search_image_view" />    <ImageView        android:id="@+id/delete_image_view"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginEnd="24dp"        android:src="@drawable/ic_delete"        app:layout_constraintBottom_toBottomOf="@id/destination_edit_text"        app:layout_constraintEnd_toEndOf="@id/where_view"        app:layout_constraintTop_toTopOf="@id/destination_edit_text" />    <ImageView        android:id="@+id/route_image_view"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="40dp"        android:layout_marginTop="26dp"        android:background="@drawable/rectangle_background"        android:backgroundTint="@color/dark_green"        android:src="@drawable/ic_route"        app:layout_constraintEnd_toStartOf="@id/wherever_image_view"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toBottomOf="@id/where_view" />    <TextView        android:id="@+id/route_text_view"        style="@style/Text2"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="8dp"        android:text="Сложный\n маршрут"        android:textColor="@color/white"        app:layout_constraintEnd_toEndOf="@id/route_image_view"        app:layout_constraintStart_toStartOf="@id/route_image_view"        app:layout_constraintTop_toBottomOf="@id/route_image_view" />    <ImageView        android:id="@+id/wherever_image_view"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="44dp"        android:background="@drawable/rectangle_background"        android:backgroundTint="@color/blue"        android:src="@drawable/ic_earth"        app:layout_constraintBottom_toBottomOf="@id/route_image_view"        app:layout_constraintEnd_toStartOf="@id/vacation_image_view"        app:layout_constraintStart_toEndOf="@id/route_image_view"        app:layout_constraintTop_toTopOf="@id/route_image_view" />    <TextView        android:id="@+id/wherever_text_view"        style="@style/Text2"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="8dp"        android:text="Куда угодно"        android:textColor="@color/white"        app:layout_constraintEnd_toEndOf="@id/wherever_image_view"        app:layout_constraintStart_toStartOf="@id/wherever_image_view"        app:layout_constraintTop_toBottomOf="@id/wherever_image_view" />    <ImageView        android:id="@+id/vacation_image_view"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="44dp"        android:background="@drawable/rectangle_background"        android:backgroundTint="@color/dark_blue"        android:src="@drawable/ic_calendar"        app:layout_constraintBottom_toBottomOf="@id/route_image_view"        app:layout_constraintEnd_toStartOf="@id/tickets_image_view"        app:layout_constraintStart_toEndOf="@id/wherever_image_view"        app:layout_constraintTop_toTopOf="@id/wherever_image_view" />    <TextView        android:id="@+id/vacation_text_view"        style="@style/Text2"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="8dp"        android:text="Выходные"        android:textColor="@color/white"        app:layout_constraintEnd_toEndOf="@id/vacation_image_view"        app:layout_constraintStart_toStartOf="@id/vacation_image_view"        app:layout_constraintTop_toBottomOf="@id/vacation_image_view" />    <ImageView        android:id="@+id/tickets_image_view"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="44dp"        android:layout_marginEnd="40dp"        android:background="@drawable/rectangle_background"        android:backgroundTint="@color/red"        android:src="@drawable/ic_fire"        app:layout_constraintBottom_toBottomOf="@id/route_image_view"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toEndOf="@id/vacation_image_view"        app:layout_constraintTop_toTopOf="@id/route_image_view" />    <TextView        android:id="@+id/tickets_text_view"        style="@style/Text2"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="8dp"        android:text="Горячие\n билеты"        android:textColor="@color/white"        app:layout_constraintEnd_toEndOf="@id/tickets_image_view"        app:layout_constraintStart_toStartOf="@id/tickets_image_view"        app:layout_constraintTop_toBottomOf="@id/tickets_image_view" />    <View        android:id="@+id/cities_view"        android:layout_width="match_parent"        android:layout_height="220dp"        android:layout_marginHorizontal="16dp"        android:layout_marginTop="26dp"        android:background="@drawable/rounded_corner"        app:layout_constraintTop_toBottomOf="@id/route_text_view" />    <ImageView        android:id="@+id/stanbul_image_view"        android:layout_width="40dp"        android:layout_height="40dp"        android:layout_marginStart="16dp"        android:layout_marginTop="24dp"        android:background="@drawable/cities_bg"        android:clipToOutline="true"        android:scaleType="centerCrop"        android:src="@drawable/ic_picture3"        app:layout_constraintStart_toStartOf="@id/cities_view"        app:layout_constraintTop_toTopOf="@id/cities_view" />    <TextView        android:id="@+id/stanbul_text_view"        style="@style/Title3"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="8dp"        android:text="Стамбул"        android:textColor="@color/white"        app:layout_constraintStart_toEndOf="@id/stanbul_image_view"        app:layout_constraintTop_toTopOf="@id/stanbul_image_view" />    <TextView        android:id="@+id/stanbul_way_text_view"        style="@style/Text2"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="4dp"        android:text="Популярное направление"        android:textColor="@color/grey_4"        app:layout_constraintStart_toStartOf="@id/stanbul_text_view"        app:layout_constraintTop_toBottomOf="@id/stanbul_text_view" />    <View        android:id="@+id/stanbul_view"        android:layout_width="0dp"        android:layout_height="1dp"        android:layout_marginHorizontal="16dp"        android:layout_marginTop="8dp"        android:background="@color/grey_3"        app:layout_constraintEnd_toEndOf="@id/cities_view"        app:layout_constraintStart_toStartOf="@id/cities_view"        app:layout_constraintTop_toBottomOf="@id/stanbul_image_view" />    <ImageView        android:id="@+id/sochi_image_view"        android:layout_width="40dp"        android:layout_height="40dp"        android:layout_marginTop="16dp"        android:background="@drawable/cities_bg"        android:clipToOutline="true"        android:scaleType="centerCrop"        android:src="@drawable/ic_picture1"        app:layout_constraintStart_toStartOf="@id/stanbul_view"        app:layout_constraintTop_toBottomOf="@id/stanbul_view" />    <TextView        android:id="@+id/sochi_text_view"        style="@style/Title3"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="8dp"        android:text="Сочи"        android:textColor="@color/white"        app:layout_constraintStart_toEndOf="@id/sochi_image_view"        app:layout_constraintTop_toTopOf="@id/sochi_image_view" />    <TextView        android:id="@+id/sochi_way_text_view"        style="@style/Text2"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="4dp"        android:text="Популярное направление"        android:textColor="@color/grey_4"        app:layout_constraintStart_toStartOf="@id/sochi_text_view"        app:layout_constraintTop_toBottomOf="@id/sochi_text_view" />    <View        android:id="@+id/sochi_view"        android:layout_width="0dp"        android:layout_height="1dp"        android:layout_marginTop="8dp"        android:background="@color/grey_3"        app:layout_constraintEnd_toEndOf="@id/stanbul_view"        app:layout_constraintStart_toStartOf="@id/stanbul_view"        app:layout_constraintTop_toBottomOf="@id/sochi_image_view" />    <ImageView        android:id="@+id/pxuket_image_view"        android:layout_width="40dp"        android:layout_height="40dp"        android:layout_marginTop="16dp"        android:background="@drawable/cities_bg"        android:clipToOutline="true"        android:scaleType="centerCrop"        android:src="@drawable/ic_picture2"        app:layout_constraintStart_toStartOf="@id/stanbul_view"        app:layout_constraintTop_toBottomOf="@id/sochi_view" />    <TextView        android:id="@+id/pxuket_text_view"        style="@style/Title3"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginStart="8dp"        android:text="Пхукет"        android:textColor="@color/white"        app:layout_constraintStart_toEndOf="@id/pxuket_image_view"        app:layout_constraintTop_toTopOf="@id/pxuket_image_view" />    <TextView        android:id="@+id/pxuket_way_text_view"        style="@style/Text2"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="4dp"        android:text="Популярное направление"        android:textColor="@color/grey_4"        app:layout_constraintStart_toStartOf="@id/pxuket_text_view"        app:layout_constraintTop_toBottomOf="@id/pxuket_text_view" />    <View        android:id="@+id/pxuket_view"        android:layout_width="0dp"        android:layout_height="1dp"        android:layout_marginTop="8dp"        android:background="@color/grey_3"        app:layout_constraintEnd_toEndOf="@id/stanbul_view"        app:layout_constraintStart_toStartOf="@id/stanbul_view"        app:layout_constraintTop_toBottomOf="@id/pxuket_image_view" />    <FrameLayout        android:id="@+id/fragment_container"        android:layout_width="match_parent"        android:layout_height="match_parent" /></androidx.constraintlayout.widget.ConstraintLayout>